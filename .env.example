# Environment Variables for Taxi Buddy
# Copy this file to .env and update the values

# Security key for user registration
VITE_REGISTRATION_SECURE_KEY=2019+

# Wear and tear rate in LKR per km
VITE_WEAR_AND_TEAR_RATE=15

# Allowed hosts for Vite preview (comma-separated)
# VITE_ALLOWED_HOSTS=example.com,localhost

# PocketBase URL (if needed)
# VITE_POCKETBASE_URL=http://127.0.0.1:8090
